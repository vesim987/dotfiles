{
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "initializationOptions": {
         "cache": {
           "directory": ".ccls-cache"
         }
       }
    },
    "MPLS": {
          "command": "/usr/bin/dotnet",
          "args": ["exec", "/home/vesim/python-language-server/output/bin/Debug/Microsoft.Python.LanguageServer.dll"],
          "filetypes": ["python"],
          "initializationOptions": {
              "interpreter": {
              "properties": {
            "InterpreterPath": "/usr/bin/python3",
            "Version": "3.7"
          }    }
              }
      }
  }
}
